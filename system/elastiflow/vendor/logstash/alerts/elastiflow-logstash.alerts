groups:
- name: elastiflow-logstash.alerts
  rules:
  - alert: ElastiflowLogstashPodMissing
    expr: 1 > 0
    for: 15m
    labels:
      context: logstash
      service: elastiflow
      severity: info
      tier: os
    annotations:
      description: {{ $labels.app }} node(s) in controlplane cluster missing, please check
      summary: {{ $labels.app }} node(s) missing